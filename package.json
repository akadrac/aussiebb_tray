{
  "name": "aussiebb_tray",
  "version": "1.0.0",
  "description": "Display your monthly useage and data left.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=releases",
    "package-win": "electron-packager . --overwrite --platform=win32 --icon=aussie_icon.ico --prune=true --out=releases --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"AussieBB Tray App\"",    
    "package-linux": "electron-packager . --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=releases"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Luke Alford",
  "license": "MIT",
  "dependencies": {
    "electron": "~1.6.2",
    "electron-store": "^1.3.0",
    "moment": "^2.18.1",
    "puppeteer": "^1.3.0",
    "request": "^2.81.0"
  },
  "devDependencies": {
    "electron-packager": "^12.0.1"
  }
}
